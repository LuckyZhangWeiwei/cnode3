    "npm-run-all": "^4.1.3",
    "query-string": "^4.3.2",
    "react": "^16.4.2",
    "react-app-rewired": "^1.5.2",
    "react-dom": "^16.4.2",
    "react-hot-component-loader": "^1.0.3",
    "react-lazy-image": "^1.1.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-simplemde-editor": "^3.6.22",
